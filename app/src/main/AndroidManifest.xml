<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nitrr.cse.incredible.eduone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    android:hardwareAccelerated="true"
    <application
        android:allowBackup="true"
        android:icon="@drawable/reg1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_Register.Register" />
        <activity android:name=".Login_Register.Login" />
        <activity android:name=".Gallery" />
        <activity android:name=".Subject.Mysubject" />
        <activity android:name=".Courses.CourseReg" />
        <activity android:name=".Courses.Student" />
        <activity android:name=".Courses.Faculty" />
        <activity android:name=".Cs.Ds.DsActivity" />
        <activity android:name=".Sub.Cs" />
        <activity android:name=".Sub.Mech" />
        <activity android:name=".Sub.Elex" />
        <activity android:name=".Sub.Civil" />
        <activity android:name=".Sub.Electrical" />
        <activity android:name=".Doubt" />
        <activity android:name=".Developer"></activity>
    </application>

</manifest>